    <script type = "text/javascript">
        
         $(document).ready(function(){
            $('.collapsible').collapsible();
        });
        
          $(document).ready(function(){
            $('.collapsible').sideNav("show");
        });
          $(document).ready(function(){
            $('.collapsible').sideNav("hide");
        });
        
//      var options = [
//        {selector: '.card', offset: 200, callback: 'globalFunction()' },
//      ];
//      scrollFire(options);
        
        $(document).ready(function(){
            $('.collapsible').collapsible({
              accordion : false // A setting that changes the collapsible behavior to expandable instead of the default accordion style
            });
        });
        
        $('.right-panel-collapsable').click(function(){
              $(".right-panel-collapsable").removeClass("blue-grey darken-2 ").addClass(" blue-grey darken-3"); // remove the prev pressed button class
              $(this).removeClass(" blue-grey darken-3").addClass("blue-grey darken-2");  
        });
        
         $(document).ready(function(){
            $('#modal1').closeModal();
          });
        
        $(document).ready(function(){
            $(".dropdown-button").dropdown({ hover: false });
        });
        
        
         $('.modal-trigger').leanModal({
              dismissible: true, // Modal can be dismissed by clicking outside of the modal
              opacity: .5, // Opacity of modal background
              in_duration: 300, // Transition in duration
              out_duration: 200, // Transition out duration
//              ready: function() { alert('Ready'); }, // Callback for Modal open
//              complete: function() { alert('Closed'); } // Callback for Modal close
            }
          );
        

        
      
      
    </script>

